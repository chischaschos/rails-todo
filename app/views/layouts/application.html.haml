!!!
%html
  %head
    %title Todo
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    %div
      - if user_signed_in?
        = link_to 'Logout', destroy_user_session_path, method: :delete

    %p.notice= notice
    %p.alert= alert

    = yield
